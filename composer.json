{
    "name": "shockedplot7560/generator",
    "description": "Generate stubs from any PHP code for IDE completion and static analysis.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "IDE",
        "static analysis"
    ],
    "homepage": "https://github.com/ShockedPlot7560/generator",
    "authors": [
        {
            "name": "Giaco Corsiglia",
            "email": "GiacoCorsiglia@gmail.com"
        }, {
            "name": "ShockedPlot7560",
            "email": "no-reply@tchallon.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "StubsGenerator\\": "src/"
        }
    },
    "bin": [
        "bin/generate-stubs"
    ],
    "require": {
        "php": "^8.2",
        "nikic/php-parser": "^4.17",
        "symfony/console": "^6.3",
        "symfony/filesystem": "^6.3",
        "symfony/finder": "^6.3"
    },
    "require-dev": {
        "vimeo/psalm": "^5.15",
        "phpunit/phpunit": "^10.3",
        "friendsofphp/php-cs-fixer": "^v3.26"
    }
}
